{
  "name": "movie-graphql-api",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "ts-node": "3.3.0",
    "@types/node": "^8.0.29",
    "typescript": "3.3.3333"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.6",
    "@types/express-graphql": "^0.9.0",
    "@types/graphql": "^14.5.0",
    "bcrypt": "^5.0.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^15.1.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.3.0",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.25"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon src/index.ts",
    "db-kill": "docker kill pg-movies && docker rm pg-movies",
    "db-start": "docker run --name pg-movies -p 5432:5432 -e POSTGRES_DB=movies -e POSTGRES_PASSWORD=secret -d postgres",
    "db-restart": "docker restart pg-movies",
    "db-console": "docker exec -it pg-movies psql -U postgres -d movies"
  }
}
