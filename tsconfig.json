{
  "compilerOptions": {
    "lib": ["es5", "es6", "ESNext"],
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    // "noUnusedLocals": true,
    "outDir": "./build",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"]
    }
  }
}
